{
    "cSpell.import": [
        "${workspaceFolder}/.vscode/cspell.yaml"
    ],
    "editor.rulers": [ 125 ],
    "git.openRepositoryInParentFolders": "always",
    "go.lintFlags": ["--fast"],
    "go.lintOnSave": "package",
    "go.lintTool": "golangci-lint",
    "go.testTimeout": "10m",
    "files.associations": {
        "*.bicept": "go-template"
      }
}
