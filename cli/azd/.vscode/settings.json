{
    "go.testTimeout": "10m",
    "go.lintTool": "golangci-lint",
    "go.lintFlags": ["--fast"],
    "go.lintOnSave": "package",
    "editor.rulers": [ 125 ],
    "cSpell.import": [
        "${workspaceFolder}/.vscode/cspell.yaml"
    ],
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#ab0326",
        "activityBar.background": "#ab0326",
        "activityBar.foreground": "#e7e7e7",
        "activityBar.inactiveForeground": "#e7e7e799",
        "activityBarBadge.background": "#239d03",
        "activityBarBadge.foreground": "#e7e7e7",
        "commandCenter.border": "#e7e7e799",
        "sash.hoverBorder": "#ab0326",
        "statusBar.background": "#79021b",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#ab0326",
        "statusBarItem.remoteBackground": "#79021b",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.activeBackground": "#79021b",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#79021b99",
        "titleBar.inactiveForeground": "#e7e7e799"
    },
    "peacock.remoteColor": "#79021b"
}
