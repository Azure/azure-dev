{
  "name": "react-fluent",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fluentui/react": "^8.73.0",
    "@microsoft/applicationinsights-react-js": "^3.3.4",
    "@microsoft/applicationinsights-web": "^2.8.4",
    "axios": "^0.27.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.3.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "envconfig": "node entrypoint.js -e .env -o ./public/env-config.js",
    "prestart": "npm run envconfig",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "shx cp ./entrypoint.sh ./build",
    "pretest": "npm run envconfig",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src --ext .ts,.tsx"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.41",
    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.17.0",
    "eslint-config-react-app": "^7.0.1",
    "if-node-version": "^1.1.1",
    "immer": "^9.0.14",
    "react-scripts": "^5.0.1",
    "shx": "^0.3.4",
    "typescript": "^4.7.3"
  },
  "overrides": {
    "async": "^3.2.3",
    "nth-check": "^2.0.1",
    "ejs": "^3.1.7",
    "svgo": "^2.8.0",
    "react-scripts": {
      "react-dev-utils": {
        "recursive-readdir": {
          "minimatch": "^3.0.5"
        }
      }
    }
  }
